@using CWDocMgr.Models
@model UploadDocsViewModel
@{
    ViewData["Title"] = "CWDocs Upload Page";
    Layout = "_Layout";
}

<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="UploadDoc">
    <br>
    <div>
        <div class="row">
            <div class="col-4 text-center">
                <input type="file" name="files" multiple/>
            </div>
            <div class="col-4 text-center">
                <button type="submit">Upload</button>
            </div>
        </div>
    </div>
</form>

